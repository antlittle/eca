<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="staff" namespace="/staff" extends="struts-default">
		<action name="staff_*"  class="com.eca.action.employee.StaffAction"
		method="{1}">
		<result name="StaffIndex">
			/WEB-INF/view/employee/staffList.jsp
		</result>
		<result name="AddStaff">
			/WEB-INF/view/employee/addStaff.jsp
		</result>
		<result name="intoUpdate">
			/WEB-INF/view/employee/staffUpdate.jsp
		</result>
		</action>
	</package>
</struts>